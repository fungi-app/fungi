{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "concurrently 'tsc -w -p tsconfig.json' 'tsc-alias -w -p tsconfig.json'",
    "start:watch": "nodemon",
    "start": "node ./dist/app.js",
    "dev": "concurrently 'npm:build:watch' 'npm:start:watch'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fungi-app/fungi.git"
  },
  "author": "Fungi Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fungi-app/fungi/issues"
  },
  "homepage": "https://github.com/fungi-app/fungi#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "tsc-alias": "^1.8.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@trpc/server": "^10.14.0",
    "express": "^4.18.2",
    "zod": "^3.21.2"
  },
  "nodemonConfig": {
    "ext": "js,json",
    "ignore": [
      ".git",
      "node_modules",
      "src",
      "package.json"
    ]
  }
}
