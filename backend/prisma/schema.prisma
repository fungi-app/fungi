// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// example model (table)
model Mushroom {
  id        Int      @id @default(autoincrement())
  name      String
  latinName String
  addedAt   DateTime
}
