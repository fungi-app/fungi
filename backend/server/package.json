{
  "name": "@fungi/server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build app.ts -o dist && cp dist/client/libquery_engine-*.node dist/",
    "watch": "npm run build && ncc build app.ts -w -o dist",
    "start": "nodemon",
    "dev": "concurrently 'npm:watch' 'npm:start'",
    "clean": "rm -rf .turbo node_modules dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@vercel/ncc": "^0.36.1",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@fungi/api": "file:../api",
    "express": "^4.18.2"
  }
}
