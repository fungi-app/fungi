{
  "expo": {
    "name": "fungi",
    "slug": "fungi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-1024x.png",
    "userInterfaceStyle": "light",
    "entryPoint": "./entry.js",
    "extra": {
      "useTunnel": false,
      "eas": {
        "projectId": "6f80efa3-2075-400f-8733-836343f77075"
      }
    },
    "splash": {
      "image": "./assets/logo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios-1024x.png"
    },
    "android": {
      "icon": "./assets/icons/android-legacy-1024x.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android-fg-1024x.png",
        "backgroundImage": "./assets/icons/android-bg-1024x.png"
      },
      "package": "io.github.fungi_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
